    "request-ip": "^3.3.0",
    "serve-favicon": "^2.5.0",
    "ts-node": "^10.9.1",
    "typeorm": "^0.2.24",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
